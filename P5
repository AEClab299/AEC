def copy odd lines (from file, to_file):
  with open(from file, 'r') as f1, open(to file, 'w') as f2:
    lines f1.readlines()
    odd_lines = [line for i, line in enumerate(lines) if i % 2 != 0]
    f2.writelines (odd_lines)
    print("copy successful")
from_file = 'input.txt'
to_file = 'output.txt'
copy_odd_lines (from_file, to_file)
